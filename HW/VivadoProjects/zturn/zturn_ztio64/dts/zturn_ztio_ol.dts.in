/dts-v1/; /plugin/;

/ {
  fragment@0 {
    target = <&base_fpga_region>;
    #address-cells = <1>;
    #size-cells = <1>;
    __overlay__ {
      #address-cells = <1>;
      #size-cells = <1>;

	  firmware-name = "zynq/%BIT_FILE%";

	  hm2reg_io_0: hm2-socfpga0@0x43C00000 {
		compatible = "generic-uio,ui_pdrv";
		reg = < 0x43C00000 0x00010000 >;
		interrupt-parent = <&intc>;
		interrupts = <0 29 1>;
	  };
    };
  };
};
